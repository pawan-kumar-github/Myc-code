#include<stdio.h>
int main()
{
	int n, n1, n2, i, j, sum = 0, c1 = 0, c2 = 0;
	printf("enter the no.");
	scanf("%d", &n);
	printf("enter two numbers");
	scanf("%d%d", &n1, &n2);
	for (i = 1; i <= n1; i++)
	{
		if (n1 % i == 0)
		{
			c1++;
		}
	}
	for (j = 1; j <= n2; j++)
	{
		if (n2 % j == 0)
		{
			c2++;
		}
	}
	if (c1 = 2 && c2 == 2)
	{
		if (n==n1+n2)
		{
			printf("addition possible");
		}
		else
		printf("addition not possible");
	}
	else
	printf("given numbers are not prime");
}